<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.androidside.subway" android:versionCode="1"
	android:versionName="1.0">
	<application android:label="@string/appname" android:icon="@drawable/icon">
		<activity android:name=".SubwayInfo" android:configChanges="orientation|keyboardHidden">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".ui.Station" android:configChanges="orientation|keyboardHidden" />
		<activity android:name=".ui.Tab1_ArrivalInfo"
			android:configChanges="orientation|keyboardHidden" />
		<activity android:name=".ui.Tab2_ExitInfo"
			android:configChanges="orientation|keyboardHidden" />
		<activity android:name=".ui.Tab3_StationInfo"
			android:configChanges="orientation|keyboardHidden" />
	</application>

	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
</manifest> 